<PackageConfiguration xmlns="http://www.siemens.com/automation/Openness/AddIn/Publisher/V17">
	
	<Author>Hakan Yıldızhan</Author>
	<Description>This Add-In offers offline viewing of logbook messages.</Description>
	<AddInVersion>V17</AddInVersion>

	<Product>
		<Name>Offline Logbook Viewer</Name>
		<Id>OfflineLogbookViewer</Id>
		<Version>1.0.0</Version>
	</Product>
	
	<FeatureAssembly>
		<AssemblyInfo>
			<Assembly>bin\release\Publish\Sirius.LogbookViewer.AddIn.dll</Assembly>
		</AssemblyInfo>
	</FeatureAssembly>

	<AdditionalAssemblies>
		<AssemblyInfo>
			<Assembly>bin\release\Publish\en\Sirius.LogbookViewer.AddIn.resources.dll</Assembly>
		</AssemblyInfo>
		<AssemblyInfo>
			<Assembly>bin\release\Publish\de\Sirius.LogbookViewer.AddIn.resources.dll</Assembly>
		</AssemblyInfo>
		<AssemblyInfo>
			<Assembly>bin\release\Publish\fr\Sirius.LogbookViewer.AddIn.resources.dll</Assembly>
		</AssemblyInfo>
	</AdditionalAssemblies>
	
	<DisplayInMultiuser />

	<RequiredPermissions>
		
		<TIAPermissions>
			<TIA.ReadOnly />
		</TIAPermissions>
		
		<SecurityPermissions>
			<System.Security.Permissions.FileIOPermission/>
			<Siemens.Engineering.AddIn.Permissions.ProcessStartPermission/>
		</SecurityPermissions>
	</RequiredPermissions>

</PackageConfiguration>