<PackageConfiguration xmlns="http://www.siemens.com/automation/Openness/AddIn/Publisher/V17">
	<Author>Hakan Yıldızhan</Author>
	<Description>This Add-In offers offline viewing of logbook messages.</Description>
	<AddInVersion>V17</AddInVersion>

	<Product>
		<Name>Offline Logbook Viewer</Name>
		<Id>OfflineLogbookViewer</Id>
		<Version>1.0.0</Version>
	</Product>
	<FeatureAssembly>
		<AssemblyInfo>
			<Assembly>bin\debug\Sirius.LogbookViewer.dll</Assembly>
			<Pdb>bin\debug\Sirius.LogbookViewer.pdb</Pdb>
		</AssemblyInfo>
	</FeatureAssembly>
	<DisplayInMultiuser />

	<AdditionalAssemblies>
		<AssemblyInfo>
			<Assembly>bin\debug\Sirius.LogbookViewer.UI.dll</Assembly>
			<Pdb>bin\debug\Sirius.LogbookViewer.UI.pdb</Pdb>
		</AssemblyInfo>
	</AdditionalAssemblies>
	
	<RequiredPermissions>
		<TIAPermissions>
			<TIA.ReadWrite />
		</TIAPermissions>

		<SecurityPermissions>
			<System.Security.Permissions.FileIOPermission/>
			<System.Security.Permissions.FileDialogPermission/>
			<System.Security.Permissions.UIPermission/>
			<System.Security.Permissions.SecurityPermission.UnmanagedCode/>
			<System.Security.Permissions.MediaPermission/>
		</SecurityPermissions>

	</RequiredPermissions>

